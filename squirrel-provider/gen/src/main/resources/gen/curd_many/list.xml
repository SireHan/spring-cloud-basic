function saveOrUpdateChild(order){
 	 $("#saveChild").on("click",function(){
 	 	 var bootstrapValidator = $("#childForm").data('bootstrapValidator');
   		 bootstrapValidator.validate();
   		 if(bootstrapValidator.isValid()){
	     	 //数据操作方式
	     	 var opType = $('#saveChild').data("type");
	     	 //查找是select2的字段并赋值
	     	 var obj = {};
	     	 obj.opType = opType;
	     	 var form = $("#childForm");
	     	 <#list childModel.table.allTableColumns as c >
	              <#if c.isInsert?? && c.isInsert == "1">
			 obj.${c.javaField} = form.find("input[name='${c.javaField}']").val();
				  </#if>
			</#list>
             $('#childModal').modal('hide');
             if(order!=undefined){
                 childData.splice(order-1,1,obj);
			 }else{
                 childData.push(obj);
			 }
             childTable = initChildTable(childData);
   		 }
     })
 }

 function saveOrUpdate(){
